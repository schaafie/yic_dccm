{
  /**
   * Allow a null option to be selected?
   */
  "allowNull": {
    "name": "allowNull",
    "type": Boolean,
    "value": false,
    "observer": "_getOptions"
  },

  /**
    * An array of icons by name: ```
[
  "editor:format-paint",
  "content-copy",
  "av:volume-off"
  
]```
  */
  "icons": {
    "name": "icons",
    "type": Array,
    "value": [],
    "observer": "_getOptions"
  },

  /**
   * The value of the option.
   */
  "value": {
    "name": "value",
    "type": String,
    "value": null,
    "reflectToAttribute": true,
    "notify": true
  },

  /**
   * the maximum number of options per row
   */
  "optionsPerRow": {
    "optionSize": "optionsPerRow",
    "type": Number,
    "value": 10,
    "observer": "_getOptions"
  },

  /**
    * An array of icons by name: ```
[
  "editor:format-paint",
  "content-copy",
  "av:volume-off"
  
]```
  */

  "__iconList": {
    "name": "__iconList",
    "type": Array,
    "read-only": true,
    "observer": "_getOptions"
  }
}
